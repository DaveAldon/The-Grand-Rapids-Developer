{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/ibm-plex-sans-var-italic.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/DavidCrawford.pdf",
      "destination": "https://www.linkedin.com/in/davidcrawfordprofile/"
    },
    {
      "source": "/resume",
      "destination": "https://www.linkedin.com/in/davidcrawfordprofile/"
    },
    {
      "source": "/projects",
      "destination": "/about"
    },
    {
      "source": "/30",
      "destination": "https://calendly.com/<profile>/30min"
    },
    {
      "source": "/15",
      "destination": "https://calendly.com/<profile>/15min"
    },
    {
      "source": "/youtube",
      "destination": "https://www.youtube.com/"
    },
    {
      "source": "/live",
      "destination": "https://www.youtube.com/channel/<channel>/live"
    },
    {
      "source": "/newsletter/(.*)",
      "destination": "/newsletter"
    }
  ]
}
